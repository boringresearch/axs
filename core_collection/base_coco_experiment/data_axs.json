{
    "script_output": [ "^^", "load_json", [ [ "^^", "get_path_from", "output_file_name" ] ] ],
    "detection_results": [ "^^", "dig", "script_output.detections" ],
    "detection_times": [ "^^", "dig", "script_output.times" ],

    "coco_detection_list_file_name": "coco_detection_list.json",
    "stored_coco_detection_list": [ "^^", "save_to_json", [
        [ "^^", "generate_coco_detection_list" ],
        [ "^^", "get_path_from", "coco_detection_list_file_name" ]
    ] ],

    "coco_annotation_entry": [ "^", "byquery", "extracted,coco_annotation" ],

    "_producer_rules": [
        [ [ "script_output", "postprocess_coco" ], [ "^^", "get", "__entry__" ], "run", { "return_saved_record_entry": true } ]
    ],

    "pycocotools_query":["python_package", "package_name=pycocotools"],
    "_BEFORE_CODE_LOADING": [ "^^", "execute", [[
        ["get_kernel"],
        ["byquery", [[ "^^", "get", "pycocotools_query"] ]],
        ["use"]
    ]] ],
    "postprocess_file_name_2": "postprocess_output.json",

    "num_of_images": 20,

    "times_file_path": [ "^^", "get_path", "script_output.json"],
    "annotations_dir": [ "^", "execute", [[
        ["byquery", "extracted,coco_annotation"],
        ["get_path", [["annotations", "instances_val2017.json"]]]
    ]] ],
    "preprocessed_files": [ "^", "execute", [[
        ["byquery", "preprocessed,coco_images,resolution=1200,first_n=20"],
        ["get_path", [["preprocessed", "original_dimensions.txt"]] ]
    ]] ]
}
