{
    "_producer_rules": [
        [ [ "python_package", "package_name==mlperf_loadgen" ], [["install"]], {
                "installable": [ "AS^IS", "^", "execute", [[
                    [ "byquery", "package_wheel,package_name=mlperf_loadgen" ],
                    [ "get_path", "*.whl" ]
                ]] ]
            }
        ],
        [ [ "python_package", "package_name==torchvision", "with_cuda" ], [["install"]], { "package_version": "0.12.0+cu113", "pip_options": "torch==1.11.0+cu113 -f https://download.pytorch.org/whl/cu113/torch_stable.html"} ],
        [ [ "python_package", "package_name==onnxruntime-gpu", "for_python36" ], [["install"]], { "package_version": "1.10.0", "pip_options": "protobuf==3.19.4" } ],
        [ [ "python_package", "package_name==onnxruntime", "for_python36" ], [["install"]], { "package_version": "1.10.0", "pip_options": "protobuf==3.19.4" } ],
        [ [ "python_package", "no_deps" ], [["install"]], { "pip_options": [ "no-deps" ] } ],
        [ [ "python_package", "special", "broken_rule" ], [["install"]], {} ],
        [ [ "python_package" ], [["install"]], {} ]
    ],

    "tool_entry": [ "^", "byquery", "shell_tool,can_python" ]
}
