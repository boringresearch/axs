{
    "_parent_entries": [ [ "^", "byname", "python_script" ] ],

    "_producer_rules": [
        [ [ "script_output", "classified_imagenet" ], [ "^^", "get", "__entry__" ], "run", { "return_saved_record_entry": true } ]
    ],

    "imagenet_query": [ "extracted", "imagenet" ],

    "imagenet_dir": [ "^", "execute", [[
        [ "byquery", [[ "^^", "get", "imagenet_query" ]] ],
        [ "get_path" ]
    ]] ],
    "num_of_images": 20,
    "model_name": "resnet50",
    "output_file_name": "script_output.json",
    "execution_device": "",
    "max_batch_size": 25,
    "top_n_max": 10,

    "class_names_path": [ "^", "execute", [[
            [ "byname", "imagenet_meta" ],
            [ "get", "class_names_path" ]
    ]] ],

    "output_file_path": [ "^^", "execute", [[
        [ "get", "__record_entry__" ],
        [ "attach", [ "^", "work_collection" ] ],
        [ "plant", [ "^^", "substitute", [[
            "tags", [ "script_output", "classified_imagenet" ],
            "num_of_images", "#{num_of_images}#",
            "model_name", "#{model_name}#",
            "output_file_name", "#{output_file_name}#",
            "script_output", [ "AS^IS", "AS^IS", "^^", "load_json", [[ "^^", "get_path_from", "output_file_name" ]] ],
            "accuracy", [ "AS^IS", "AS^IS", "^^", "execute", [[
                [ "get_kernel" ],
                [ "byname", "imagenet_meta" ],
                [ "measure_accuracy", [[ "^^", "dig", "script_output.predictions" ]] ]
            ]] ],
            "print_top_n_predictions", [ "AS^IS", "AS^IS", "^^", "execute", [[
                [ "get_kernel" ],
                [ "byname", "imagenet_meta" ],
                [ "full_print_predictions", [[ "^^", "dig", "script_output.top_n" ]] ]
            ]] ]                                              
        ]] ] ],
        [ "save" ],
        [ "get_path_from", "output_file_name" ]
    ]] ],

    "script_extra_params": [ "^^", "substitute", "\"#{imagenet_dir}#\" #{num_of_images}# #{model_name}# \"#{output_file_path}#\" \"#{execution_device}#\" #{max_batch_size}# #{class_names_path}# \"#{top_n_max}#\"" ],

    "rel_script_path": "barebones_resnet50.py",

    "torchvision_query": [ "python_package", "package_name=torchvision" ],

    "python_deps": [
        [ "^", "byquery", [[ "^^", "get", "torchvision_query" ]] ]
    ]
}
