{
    "_parent_entries": [ [ "^", "byname", "python_script" ] ],

    "_producer_rules": [
        [ [ "script_output", "classified_imagenet", "framework=pytorch" ], [ "^^", "get", "__entry__" ], "run", { "return_saved_record_entry": true } ]
    ],

    "model_name": "resnet50",
    "resolution": 224,

    "preprocessed_images_query": [ "preprocessed", "dataset_name=imagenet", [ "^^", "substitute", "resolution=#{resolution}#" ] ],
    "preprocessed_images_dir": [ "^", "execute", [[
        [ "byquery", [[ "^^", "get", "preprocessed_images_query" ]] ],
        [ "get_path" ]
    ]], {}, ["preprocessed_images_query"] ],

    "num_of_images": 20,
    "output_file_name": "script_output.json",
    "execution_device": "",
    "max_batch_size": 25,
    "top_n_max": 10,
    "framework": "pytorch",
    "num_gpus": [ "^", "execute", [[
                        [ "byquery", "shell_tool,can_gpu" ],
                        [ "run"  ]
                ]]  ],
    "output_file_path": [ "^^", "execute", [[
        [ "get", "__record_entry__" ],
        [ "attach", [ "^", "work_collection" ] ],
        [ "plant", [ "^^", "substitute", [[
            "_parent_entries", [ "AS^IS", "AS^IS", [ "^", "byname", "base_imagenet_experiment" ] ],
            "tags", [ "script_output", "classified_imagenet" ],
            "num_of_images", "#{num_of_images}#",
            "model_name", "#{model_name}#",
	        "framework", "#{framework}#",
            "preprocessed_images_dir", "#{preprocessed_images_dir}#",
            "output_file_name", "#{output_file_name}#"
        ]] ] ],
        [ "save" ],
        [ "get_path_from", "output_file_name" ]
    ]] ],

    "script_extra_params": [ "^^", "substitute", "\"#{preprocessed_images_dir}#\" #{resolution}# #{num_of_images}# #{model_name}# \"#{output_file_path}#\" \"#{execution_device}#\" #{max_batch_size}# #{top_n_max}#" ],

    "rel_script_path": "pytorch_classify.py",

    "torchvision_query": [ "^^", "case", [ [ "^^", "get", "num_gpus"] , "0" ,["python_package", "package_name=torchvision"] ], {"default_value": ["python_package", "package_name=torchvision", "with_cuda"] } ],

    "python_deps": [
        [ "^", "byquery", [[ "^^", "get", "torchvision_query" ]], {}, ["torchvision_query"] ],
        [ "^", "byquery", "data_loader,imagenet" ]
    ]
}
