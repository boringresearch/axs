{
    "_parent_entries": [ [ "^", "byname", "shell" ] ],
    "_producer_rules": [
            [ [ "compiled", "hello_world" ], [ "^^", "get", "__entry__" ], "run", { } ]
    ],
    "tags": [ "source_code", "hello_world" ],
    "file_name": "hello.c",
    "abs_source_path": [ "^^", "get_path" ],

    "return_this_entry": [ "^", "execute", [[
        [ "work_collection" ],
        [ "attached_entry", [ "compiled_hello_world", {
                "_parent_entries": [ [ "AS^IS", "AS^IS", "^", "byname", "shell" ] ],
                "tags": [ "compiled", "hello_world"] ,
                "binary_name": "hello",
                "abs_binary_path": ["AS^IS", "AS^IS", "^^", "get_path_from", "binary_name"],
                "shell_cmd_with_subs": "#{abs_binary_path}#"
            } ] ],
        [ "save" ]
    ]] ],

    "compiler_entry": [ "^", "byquery", "shell_tool,can_compile_c" ],

    "shell_cmd_with_subs": "#{compiler_entry.tool_path}# -o #{return_this_entry.abs_binary_path}# #{abs_source_path}#"
}
