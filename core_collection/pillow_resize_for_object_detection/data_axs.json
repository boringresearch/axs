{
    "_producer_rules": [
        [ [ "preprocessed", "dataset_name=coco" ], [ "^^", "get", "__entry__" ], "preprocess", {
            "images_directory": [ "AS^IS", "^", "execute", [[
                [ "byquery", [[ "^^", "get", "coco_images_query" ]] ], 
                [ "get_path" ]
                ]], {}, [ "coco_images_query" ]],
            "images_annotation": [ "AS^IS", "^^", "execute", [[
                [ "load_json", [[ "^^", "get", "coco_annotation_path" ]] ],
                [ "get", "images" ]
            ]] ]
          },
          [
            "images_directory", "resolution", "supported_extensions",
            "data_type","new_file_extension", "file_name", "fof_name"
          ]
        ],
        [ [ "preprocessed", "dataset_name=openimages" ], [ "^^", "get", "__entry__" ], "preprocess", {
            "images_directory": [ "AS^IS", "^", "execute", [[
                [ "byquery", [[ "^^", "get", "openimages_query" ]] ], 
                [ "get_path" , [["validation", "data"]] ]
                ]], {}, [ "openimages_query" ]],
            "images_annotation": ""
        },
        [
            "images_directory", "resolution", "supported_extensions",
            "data_type","new_file_extension", "file_name", "fof_name"
          ]
        ]
    ],

    "coco_annotation_path": [ "^", "execute", [[
        [ "byquery", "extracted,coco_annotation" ],
        [ "get_path", [["annotations", "instances_val2017.json"]] ]
    ]] ],

    "numpy_query": [ "python_package", "package_name=numpy" ],
    "pillow_query": [ "python_package", "package_name=pillow" ],
    "_BEFORE_CODE_LOADING": [ "^^", "execute", [[
        ["get_kernel"],
        ["byquery", [[ "^^", "get", "numpy_query" ]] ],
        ["use"],
        [],
        ["get_kernel"],
        ["byquery", [[ "^^", "get", "pillow_query" ]] ],
        ["use"]
    ]] ],

    "images_directory": "",
    "coco_images_query": [ "extracted", "coco_images" ],
    "openimages_query": [ "downloaded", "openimages_mlperf" ],

    "resolution":           1200,
    "supported_extensions": ["jpeg", "jpg", "gif", "png"],
    "data_type":            "uint8",
    "new_file_extension":   "rgb8",
    "file_name":            "preprocessed",
    "fof_name":             "original_dimensions.txt",
    "first_n":              null
}
