{
    "_producer_rules": [
        [ [ "preprocessed", "imagenet" ], [ "^^", "get", "__entry__" ], "preprocess", {} ]
    ],

    "_parent_entries": [ [ "^", "byname", "python_script" ] ],

    "numpy_query": [ "python_package", "package_name=numpy" ],
    "pillow_query": [ "python_package", "package_name=pillow" ],

    "_BEFORE_CODE_LOADING": [ "^^", "execute", [[
        ["get_kernel"],
        ["byquery", [ "^^", "get", "numpy_query" ]],
        ["use"],
        ["get_kernel"],
        ["byquery", [[ "^^", "get", "pillow_query" ]] ],
        ["use"]
    ]] ],

    "imagenet_query": [ "extracted", "imagenet500" ],

    "imagenet_directory": [ "^", "execute", [[
        [ "byquery", [[ "^^", "get", "imagenet_query" ]] ],
        [ "get_path" ]
    ]] ],
    "square_size":          224,
    "supported_extensions": ["jpeg", "jpg", "gif", "png"],
    "crop_percentage":      87.5,
    "inter_size":           0,
    "convert_to_bgr":       false,
    "data_type":            "uint8",
    "new_file_extension":   "rgb8",
    "file_name":            "preprocessed"
}
